
## <summary>policy for tavecchiacoinpay</summary>


########################################
## <summary>
##	Transition to tavecchiacoinpay.
## </summary>
## <param name="domain">
## <summary>
##	Domain allowed to transition.
## </summary>
## </param>
#
interface(`tavecchiacoinpay_domtrans',`
	gen_require(`
		type tavecchiacoinpay_t, tavecchiacoinpay_exec_t;
	')

	corecmd_search_bin($1)
	domtrans_pattern($1, tavecchiacoinpay_exec_t, tavecchiacoinpay_t)
')


########################################
## <summary>
##	Execute tavecchiacoinpay server in the tavecchiacoinpay domain.
## </summary>
## <param name="domain">
##	<summary>
##	Domain allowed access.
##	</summary>
## </param>
#
interface(`tavecchiacoinpay_initrc_domtrans',`
	gen_require(`
		type tavecchiacoinpay_initrc_exec_t;
	')

	init_labeled_script_domtrans($1, tavecchiacoinpay_initrc_exec_t)
')


