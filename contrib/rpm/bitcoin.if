########################################
## <summary>
##	All of the rules required to administrate
##	a tavecchiacoinpay environment
## </summary>
## <param name="domain">
##	<summary>
##	Domain allowed access.
##	</summary>
## </param>
## <param name="role">
##	<summary>
##	Role allowed access.
##	</summary>
## </param>
## <rolecap/>
#
interface(`tavecchiacoinpay_admin',`
	gen_require(`
		type tavecchiacoinpay_t;
		type tavecchiacoinpay_initrc_exec_t;
		type tavecchiacoinpay_var_lib_t;
	')

	allow $1 tavecchiacoinpay_t:process { ptrace signal_perms };
	ps_process_pattern($1, tavecchiacoinpay_t)

	tavecchiacoinpay_initrc_domtrans($1)
	domain_system_change_exemption($1)
	role_transition $2 tavecchiacoinpay_initrc_exec_t system_r;
	allow $2 system_r;

	files_search_var_lib($1)
	admin_pattern($1, tavecchiacoinpay_var_lib_t)

')

